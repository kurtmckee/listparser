<?xml version="1.0"?>
<!--
    Liferea uses type="pie" when referring to Atom feeds

    Description: //outline[@type='pie']
    Eval: result['bozo'] == 0
    Eval: len(result['feeds']) == 1
-->
<opml version="2.0">
    <head></head>
    <body>
        <outline text="feedname" type="pie" xmlUrl="http://domain.com/feed" />
    </body>
</opml>
