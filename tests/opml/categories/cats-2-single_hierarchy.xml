<?xml version="1.0"?>
<!--
    Description: //outline/@category (categories 2)
    Eval: result.bozo == 0
    Eval: len(result.feeds[0].tags) == 0
    Eval: len(result.feeds[0].categories) == 1
    Eval: ['root', 'parent', 'child'] in result.feeds[0].categories
-->
<opml version="2.0">
    <head></head>
    <body>
        <outline category="/root/parent/child" text="feedname" type="rss" xmlUrl="http://domain/feed" />
    </body>
</opml>
